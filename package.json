{
  "name": "skylark-jquery",
  "version": "0.9.6",
  "description": "The skylark plugin library for fully compatible API with jquery.",
  "main": "-",
  "scripts": {
    "build": "slib build .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "homepage": "www.skylarkjs.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skylark-integration/skylark-jquery.git"
  },
  "keywords": [
    "skylark",
    "skylarkjs",
    "jquery fully compatible",
    "library"
  ],
  "author": "Hudaokeji Co.,Ltd",
  "license": "MIT",
  "bugs": "https://github.com/skylark-integration/skylark-jquery/issues",
  "dependencies": {
    "skylark-domx-data": "git+https://github.com/skylark-domx/skylark-domx-data.git",
    "skylark-domx-eventer": "git+https://github.com/skylark-domx/skylark-domx-eventer.git",
    "skylark-domx-finder": "git+https://github.com/skylark-domx/skylark-domx-finder.git",
    "skylark-domx-forms": "git+https://github.com/skylark-domx/skylark-domx-forms.git",
    "skylark-domx-fx": "git+https://github.com/skylark-domx/skylark-domx-fx.git",
    "skylark-domx-geom": "git+https://github.com/skylark-domx/skylark-domx-geom.git",
    "skylark-domx-noder": "git+https://github.com/skylark-domx/skylark-domx-noder.git",
    "skylark-domx-plugins-base": "git+https://github.com/skylark-domx-plugins/skylark-domx-plugins-base.git",
    "skylark-domx-query": "git+https://github.com/skylark-domx/skylark-domx-query.git",
    "skylark-domx-scripter": "git+https://github.com/skylark-domx/skylark-domx-scripter.git",
    "skylark-domx-styler": "git+https://github.com/skylark-domx/skylark-domx-styler.git",
    "skylark-domx-velm": "git+https://github.com/skylark-domx/skylark-domx-velm.git",
    "skylark-langx": "git+https://github.com/skylark-langx/skylark-langx.git",
    "skylark-langx-scripter": "git+https://github.com/skylark-domx/skylark-domx-scripter.git",
    "skylark-net-http": "git+https://github.com/skylark-net/skylark-net-http.git"
  },
  "skylarkjs": {
    "directories": {
      "dependencies": "./node_modules"
    },
    "secondaries": {},
    "build": {
      "prepare": {
        "texttojs": [
          "html",
          "json",
          "css"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      },
      "bundles": {
        "initOnLoad": true,
        "all": true
      }
    }
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
